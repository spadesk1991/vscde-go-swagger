{
	"name": "go-swagger",
	"displayName": "go-swagger",
	"description": "a golang  swagger",
	"version": "0.0.1",
	 "publisher": "spadesk",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.swagger"
	],
	"main": "./extension.js",
	 "repository": {
        "type": "git",
        "url": "https://github.com/spadesk1991/vscde-go-swagger.git"
    },
	"icon": "images/icon.png",
	"contributes": {
		"commands": [
			{
				"command": "extension.swagger",
				"title": "swagger"
			}
		],
		"menus": {
			"editor/context": [
				{
  				"command": "extension.swagger",
                "when": "editorTextFocus && resourceFilename =~ /.go/"
				}
			]
		}
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	}
}
